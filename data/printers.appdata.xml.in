<?xml version="1.0" encoding="UTF-8"?>
<component type="addon">
  <id>io.elementary.switchboard.printers</id>
  <extends>io.elementary.switchboard</extends>
  <name>Printer Settings</name>
  <summary>Configure printers, manage print queues, and view ink levels</summary>
  <icon type="stock">printer</icon>
  <translation type="gettext">printers-plug</translation>
  <releases>
    <release version="2.1.11" date="2022-06-15" urgency="medium">
      <description>
        <p>Minor updates:</p>
        <ul>
          <li>Update sidebar after adding or removing printer</li>
          <li>Show the supported language for each driver</li>
          <li>Use newer add/remove list pattern<li>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>
    <release version="2.1.10" date="2021-07-21" urgency="medium">
      <description>
        <p>Minor updates:</p>
        <ul>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>
    <release version="2.1.9" date="2020-05-28" urgency="medium">
      <description>
        <p>Minor updates:</p>
        <ul>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>
    <release version="2.1.8" date="2020-04-03" urgency="medium">
      <description>
        <p>Minor updates:</p>
        <ul>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>
    <release version="2.1.7" date="2019-11-15" urgency="medium">
      <description>
        <p>Show a default name if a printer is unnamed</p>
        <p>Updated translations</p>
      </description>
    </release>
  </releases>
