  <update_contact>contact_at_elementary.io</update_contact>

  <releases>
    <release version="8.0.0" date="2024-07-02" urgency="medium">
      <description>
        <p>Minor updates:</p>
        <ul>
          <li>Improved screen reader support</li>
          <li>Provide feedback for form validation when adding printers</li>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>

    <release version="2.2.1" date="2022-10-20" urgency="medium">
      <description>
        <p>Minor updates:</p>
        <ul>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>

    <release version="2.2.0" date="2022-06-28" urgency="medium">
      <description>
        <p>New Features:</p>
        <ul>
          <li>Allow clearing the queue for each printer</li>
        </ul>
        <p>Improvements:</p>
        <ul>
          <li>Improved job row layout</li>
          <li>Show the supported language for each driver</li>
          <li>Show color names below ink levels</li>
          <li>Avoid expanding the window for printers with many ink levels</li>
          <li>Use newer add/remove list pattern</li>
        </ul>
        <p>Fixes:</p>
        <ul>
          <li>Update sidebar after adding or removing printer</li>
          <li>Update sidebar after enabling or disabling printer</li>
          <li>Update job list after job state change</li>
        </ul>
        <p>Minor updates:</p>
        <ul>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>

    <release version="2.1.10" date="2021-07-21" urgency="medium">
      <description>
        <p>Minor updates:</p>
        <ul>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>

    <release version="2.1.9" date="2020-05-28" urgency="medium">
      <description>
        <p>Minor updates:</p>
        <ul>
          <li>Updated translations</li>
        </ul>
      </description>
    </release>

    <release version="2.1.8" date="2020-04-03" urgency="medium" />
    <release version="2.1.7" date="2019-11-15" urgency="medium" />
  </releases>
</component>
